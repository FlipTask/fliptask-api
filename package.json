{
  "name": "open-trello",
  "version": "1.0.0",
  "description": "Task Management",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev-build-server": "webpack --config webpack/dev/webpack.server.js",
    "dev:build-client": "webpack --config webpack/dev/webpack.client.js --watch",
    "dev:server": "nodemon --watch server --exec \"npm run dev-build-server && node --inspect server-build/server.js\"",
    "prod": "npm-run-all --parallel prod:*",
    "prod-build": "npm-run-all --parallel prod:build-server prod:build-client",
    "prod:server": "BABEL_ENV=prod NODE_ENV=production node server-build/server.js",
    "prod:build-server": "BABEL_ENV=prod webpack --config webpack/production/webpack.server.js",
    "prod:build-client": "BABEL_ENV=prod webpack --config webpack/production/webpack.client.js"
  },
  "author": "Ashish Kumar",
  "license": "ISC",
  "dependencies": {
    "@loadable/server": "5.12.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.6",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "react-customize-token-input": "^0.5.2",
    "react-dropzone": "^11.0.1",
    "react-router-config": "^5.1.1",
    "universal-cookie": "^4.0.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
